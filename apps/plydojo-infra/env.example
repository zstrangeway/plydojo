# SST Infrastructure Environment Variables
# Copy this file to .env and fill in your actual values

# OpenAI API Key for AI tutoring
OPENAI_API_KEY=sk-your-openai-api-key-here

# JWT Secret for token signing
JWT_SECRET=your-jwt-secret-at-least-32-characters-long

# Stripe API Key for subscriptions (when needed)
STRIPE_API_KEY=sk_test_your-stripe-secret-key-here

# AWS Region (optional - defaults to us-east-1)
AWS_REGION=us-east-1

# Domain for email verification (update when custom domain is available)
EMAIL_DOMAIN=plydojo.com 